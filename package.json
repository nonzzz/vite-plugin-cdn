{
  "name": "vite-plugin-cdn2",
  "version": "0.3.0",
  "description": "A Vite plugin that allowed you replace module with CDN",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node bootstrap.js --build",
    "test": "ava",
    "dev": "node bootstrap.js",
    "coverage": "c8 -r=lcov ava"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "cdn",
    "vite-plugin-cdn"
  ],
  "author": "Kanno",
  "license": "MIT",
  "homepage": "https://github.com/nonzzz/vite-plugin-cdn",
  "bugs": {
    "url": "https://github.com/nonzzz/vite-plugin-cdn/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nonzzz/vite-plugin-cdn.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "acorn": "^8.8.0",
    "ava": "^4.3.3",
    "c8": "^7.12.0",
    "eslint": "^8.23.1",
    "eslint-config-kagura": "^1.0.3",
    "eslint-config-prettier": "^8.5.0",
    "no-bump": "^0.5.1",
    "prettier": "^2.4.1",
    "rollup-plugin-dts": "^4.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "vite": "^3.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "happy-dom": "^6.0.4",
    "magic-string": "^0.26.4"
  }
