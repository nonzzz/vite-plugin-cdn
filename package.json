{
  "name": "vite-plugin-cdn2",
  "version": "0.8.0",
  "description": "A Vite plugin that allowed you replace module with CDN",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bump",
    "test": "c8 -r=lcov ava __tests__/*.spec.ts",
    "lint": "eslint . --fix",
    "dev": "bump -w",
    "prepare:e2e": "cd e2e && npm run installer",
    "e2e": "ava e2e/**/*.spec.ts"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "cdn",
    "vite-plugin-cdn"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./url": {
      "import": "./dist/url.mjs",
      "require": "./dist/url.js"
    },
    "./*": "./*"
  },
  "author": "Kanno",
  "license": "MIT",
  "homepage": "https://github.com/nonzzz/vite-plugin-cdn",
  "bugs": {
    "url": "https://github.com/nonzzz/vite-plugin-cdn/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nonzzz/vite-plugin-cdn.git"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@types/babel__core": "^7.20.1",
    "ava": "^5.2.0",
    "c8": "^7.12.0",
    "eslint": "^8.23.1",
    "eslint-config-kagura": "^1.2.0",
    "no-bump": "^0.7.1",
    "playwright": "^1.35.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vue": "^3.2.47"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@rollup/pluginutils": "^5.0.2",
    "@types/estree": "^1.0.1",
    "happy-dom": "^6.0.4",
    "rs-module-lexer": "^1.0.0"
  },
  "ava": {
    "files": [
      "__tests__/*.spec.ts",
      "e2e/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
