{
  "name": "vite-plugin-cdn2",
  "version": "0.3.3",
  "description": "A Vite plugin that allowed you replace module with CDN",
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bump",
    "test": "ava",
    "dev": "bump -w",
    "coverage": "c8 -r=lcov ava"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "cdn",
    "vite-plugin-cdn"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Kanno",
  "license": "MIT",
  "homepage": "https://github.com/nonzzz/vite-plugin-cdn",
  "bugs": {
    "url": "https://github.com/nonzzz/vite-plugin-cdn/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nonzzz/vite-plugin-cdn.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "acorn": "^8.8.0",
    "ava": "^4.3.3",
    "c8": "^7.12.0",
    "eslint": "^8.23.1",
    "eslint-config-kagura": "^1.0.3",
    "eslint-config-prettier": "^8.5.0",
    "no-bump": "^0.7.1",
    "prettier": "^2.4.1",
    "rollup-plugin-dts": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3"
  },
  "ava": {
    "files": [
      "__tests__/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "happy-dom": "^6.0.4",
    "magic-string": "^0.26.4",
    "rs-module-lexer": "^1.0.0"
  }
}
