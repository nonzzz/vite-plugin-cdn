{
  "name": "vite-plugin-cdn2",
  "version": "0.4.2",
  "description": "A Vite plugin that allowed you replace module with CDN",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bump",
    "test": "ava __tests__/*.spec.ts",
    "lint": "eslint . --fix",
    "dev": "bump -w",
    "coverage": "c8 -r=lcov ava",
    "prepare:e2e": "cd e2e && npm run installer"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "cdn",
    "vite-plugin-cdn"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Kanno",
  "license": "MIT",
  "homepage": "https://github.com/nonzzz/vite-plugin-cdn",
  "bugs": {
    "url": "https://github.com/nonzzz/vite-plugin-cdn/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nonzzz/vite-plugin-cdn.git"
  },
  "devDependencies": {
    "ava": "^5.2.0",
    "c8": "^7.12.0",
    "eslint": "^8.23.1",
    "eslint-config-kagura": "^1.1.0",
    "no-bump": "^0.7.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vue": "^3.2.47"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@types/estree": "^1.0.1",
    "happy-dom": "^6.0.4",
    "magic-string": "^0.26.4",
    "rs-module-lexer": "^1.0.0"
  },
  "ava": {
    "files": [
      "__tests__/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
